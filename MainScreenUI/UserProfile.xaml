<Page x:Class="MainScreenUI.UserProfile"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MainScreenUI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="UserProfile" Background="#FF2D2D30">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="67*"/>
            <ColumnDefinition Width="333*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="19*"/>
            <RowDefinition Height="126*"/>
            <RowDefinition Height="305*"/>
        </Grid.RowDefinitions>
        <TextBlock Text="This is Profile" FontSize="22" Background="#FFD9D9D9" Margin="0,0,0,-0.4" Grid.RowSpan="3" Grid.ColumnSpan="2"/>
    </Grid>
</Page>
